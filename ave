function[a]=ave(a)   //未完成
   
   
    for i=1:4:256
        for j=1:4:256
            for k=1:1:3
        s=a(i,j,k);
        t=a(i+1,j,k);
        u=a(i,j+1,k);
        v=a(i+1,j+1,k);
        b=(s+t+u+v)/4;      //変数の型、個数をそろえる
        
        a(i,j,k)=b;
        a(i+1,j,k)=b;
        a(i,j+1,k)=b;
        a(i+1,j+1,k)=b;
        
        
        
    end
end
end

endfunction
